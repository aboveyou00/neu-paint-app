<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="tooearly.com.neu_paint_app.Activities.MainActivity">

    <tooearly.com.neu_paint_app.Views.PaintView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/paintView">
    </tooearly.com.neu_paint_app.Views.PaintView>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_save"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imageButton17" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_close_clear_cancel"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton17"
        android:layout_toEndOf="@+id/imageButton17"
        android:id="@+id/imageButton18" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_revert"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageButton19" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="125dp"
        android:layout_alignBottom="@+id/button5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button6"
        android:background="@color/Red" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="125dp"
        android:layout_below="@+id/button5"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:id="@+id/button7"
        android:background="@color/Green" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="125dp"
        android:layout_below="@+id/button6"
        android:layout_alignLeft="@+id/button6"
        android:layout_alignStart="@+id/button6"
        android:id="@+id/button8"
        android:background="@color/Yellow" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="125dp"
        android:layout_below="@+id/button7"
        android:layout_alignRight="@+id/button7"
        android:layout_alignEnd="@+id/button7"
        android:id="@+id/button9"
        android:background="@color/Purple" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="125dp"
        android:layout_below="@+id/button8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button10"
        android:background="@color/Orange" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="125dp"
        android:layout_below="@+id/button9"
        android:layout_alignLeft="@+id/button9"
        android:layout_alignStart="@+id/button9"
        android:id="@+id/button11"
        android:background="@color/Pink" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="125dp"
        android:layout_below="@+id/button10"
        android:layout_alignLeft="@+id/button10"
        android:layout_alignStart="@+id/button10"
        android:id="@+id/button12"
        android:background="@color/Brown" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="125dp"
        android:layout_marginTop="98dp"
        android:id="@+id/button5"
        android:background="@color/Blue"
        android:layout_below="@+id/imageButton19"
        android:layout_toLeftOf="@+id/button6"
        android:layout_toStartOf="@+id/button6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/triangle"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton17"
        android:layout_toEndOf="@+id/imageButton17"
        android:layout_marginLeft="39dp"
        android:layout_marginStart="39dp"
        android:id="@+id/imageButton20" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/line"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton20"
        android:layout_marginBottom="30dp"
        android:layout_toEndOf="@+id/imageButton20"
        android:id="@+id/imageButton21" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/circle"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/imageButton21"
        android:layout_toEndOf="@+id/imageButton21"
        android:id="@+id/imageButton22" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_popup_sync"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton24"
        android:layout_toEndOf="@+id/imageButton24"
        android:id="@+id/imageButton25" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_popup_sync"
        android:id="@+id/imageButton28"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8"
        android:layout_marginLeft="58dp"
        android:layout_marginStart="58dp" />

    <TextView
        android:text="Vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton28"
        android:layout_alignLeft="@+id/imageButton28"
        android:layout_alignStart="@+id/imageButton28"
        android:id="@+id/textView9" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_popup_sync"
        android:id="@+id/imageButton27"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_popup_sync"
        android:id="@+id/imageButton26"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton18"
        android:layout_toEndOf="@+id/imageButton18"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp" />

    <TextView
        android:text="Undo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_below="@+id/imageButton19"
        android:layout_alignLeft="@+id/imageButton19"
        android:layout_alignStart="@+id/imageButton19"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp" />

    <TextView
        android:text="Horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:layout_below="@+id/imageButton27"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Invert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_below="@+id/imageButton26"
        android:layout_alignLeft="@+id/imageButton26"
        android:layout_alignStart="@+id/imageButton26" />


</RelativeLayout>
